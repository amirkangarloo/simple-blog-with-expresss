<hr class="invis">
<section class="section">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <div class="page-wrapper">
    <div class="blog-top clearfix">
        <h4 class="pull-left"><a><i class="fa fa-search"></i></a> Result of {{keyword}} is : </h4>
    </div><!-- end blog-top -->

    <div class="blog-list clearfix">

        {{#each post}}

        <div class="blog-box row">
            <div class="col-md-4">
                <div class="post-media">
                    <a href="/blog/{{this.slug}}" title="">
                        <img src="/static/upload/thumbnails/{{this.thumbnail}}" alt="" class="img-fluid">
                        <div class="hovereffect"></div>
                    </a>
                </div><!-- end media -->
            </div><!-- end col -->

            <div class="blog-meta big-meta col-md-8">
                <h4><a href="/blog/{{this.slug}}" title="">{{this.title}}</a></h4>
                <p> {{{this.summary}}} </p>
                <small><a href="tech-single.html" title="">{{this.created}}</a></small>
                <small><a href="tech-author.html" title="">by {{this.full_name}}</a></small>
                <small><a href="tech-single.html" title=""><i class="fa fa-eye"></i>
                        {{this.views}}</a></small>
            </div><!-- end meta -->
        </div><!-- end blog-box -->
        <hr class="invis">

        {{/each}}

    </div><!-- end blog-list -->
</div><!-- end page-wrapper -->

                
            </div><!-- end col -->

            
        </div><!-- end row -->
    </div><!-- end container -->
</section>